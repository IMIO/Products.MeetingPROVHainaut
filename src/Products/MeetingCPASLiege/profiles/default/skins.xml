<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="meetingcpasliege_styles"
         meta_type="Filesystem Directory View"
         directory="MeetingCPASLiege/skins/meetingcpasliege_styles"/>

 <object name="meetingcpasliege_templates"
         meta_type="Filesystem Directory View"
         directory="MeetingCPASLiege/skins/meetingcpasliege_templates"/>
 
 <skin-path name="*">
  <layer insert-after="custom"
         name="meetingcpasliege_styles"/>
  <layer insert-after="custom"
         name="meetingcpasliege_templates"/>
 </skin-path>

</object>
